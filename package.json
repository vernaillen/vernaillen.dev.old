    "@nuxt/ui": "^2.15.2",
    "@nuxthq/studio": "^1.0.15",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/plausible": "^1.0.0",
    "@types/node": "^20.12.8",
    "@vernaillen/nuxt-base-layer": "^0.3.5",
