{
  "name": "vernaillen.dev",
  "version": "1.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/vernaillen/vernaillen.dev.git"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lagon:dev": "NITRO_PRESET=lagon NUXT_IMAGE_PROVIDER=twicpics pnpm build && npx -p esbuild -p @lagon/cli lagon dev",
    "lagon:deploy": "NITRO_PRESET=lagon NUXT_IMAGE_PROVIDER=twicpics pnpm build && npx -p esbuild -p @lagon/cli lagon deploy",
    "vercel:deploy": "NITRO_PRESET=vercel-edge NUXT_IMAGE_PROVIDER=twicpics vercel deploy",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:dev": "start-server-and-test dev http://localhost:3000 'cypress open --config baseUrl=http://localhost:3000'",
    "cy:local": "start-server-and-test preview http://localhost:3000 'cypress run --config baseUrl=http://localhost:3000'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "unlighthouse:dev": "start-server-and-test preview http://localhost:3000 'npx unlighthouse --site localhost:3000'",
    "unlighthouse:prod": "npx unlighthouse",
    "fulltest": "rm -rf node_modules && pnpm i && pnpm run lint && pnpm run build && pnpm run coverage && pnpm run cy:local && pnpm run unlighthouse:dev"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "consola": "^3.2.3",
    "masto": "^6.5.1",
    "nuxt-time": "^0.1.1",
    "resend": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "vee-validate": "^4.11.8",
    "vue-easy-lightbox": "^1.16.0",
    "vue-recaptcha-v3": "^2.0.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@egoist/tailwindcss-icons": "^1.4.0",
    "@iconify-json/logos": "^1.1.38",
    "@iconify-json/lucide": "^1.1.141",
    "@iconify-json/material-symbols": "^1.1.63",
    "@iconify-json/mdi": "^1.1.55",
    "@iconify-json/twemoji": "^1.1.12",
    "@iconify-json/uil": "^1.1.5",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.3",
    "@nuxt/image": "1.1.0",
    "@nuxt/ui": "^2.10.0",
    "@nuxthq/studio": "^1.0.5",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/plausible": "^0.2.3",
    "@nuxtjs/tailwindcss": "^6.10.0",
    "@types/node": "^20.9.4",
    "@vernaillen/nuxt-base-layer": "^0.2.21",
    "@vitest/coverage-v8": "^0.34.6",
    "@vueuse/nuxt": "^10.6.1",
    "cypress": "^13.6.0",
    "dayjs": "^1.11.10",
    "nuxt": "3.8.2",
    "nuxt-svgo": "^3.6.0",
    "prism-theme-vars": "^0.2.4",
    "remark-breaks": "^4.0.0",
    "remark-directive-rehype": "^0.4.2",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
